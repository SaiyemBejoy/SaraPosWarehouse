@{
    ViewBag.Title = "About";
}
<style>
    .bg_image {
        height: 85vh;
        background-image: url("/Content/img/IndexImg/POS Banner_8.jpg");
        background-repeat: round !important;
    }
</style>
<div class="col-lg-12 col-sm-12 col-md-12 col-xs-12 bg_image"></div>
   
<script>
    jQuery(document).ready(function () {
        @{
            if (!string.IsNullOrWhiteSpace(@ViewBag.MenuPermisionMessage))
            {
                <text>
                    var message = '@ViewBag.MenuPermisionMessage';


                    toastr.options = {
                        "closeButton": true,
                        "debug": false,
                        "positionClass": "toast-top-center",
                        "onclick": null,
                        "showDuration": "1000",
                        "hideDuration": "1000",
                        "timeOut": "5000",
                        "extendedTimeOut": "1000",
                        "showEasing": "swing",
                        "hideEasing": "linear",
                        "showMethod": "fadeIn",
                        "hideMethod": "fadeOut"
                    }
                    toastr.warning(message);
                </text>
            }
        }

    });
</script>